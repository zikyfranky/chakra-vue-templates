{
  "name": "chakra-vue-templates",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zikyfranky/chakra-vue-templates.git"
  },
  "author": "Isaac Frank <codingwithisaac@gmail.com>",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn lint",
      "pre-push": "yarn lint"
    }
  },
  "dependencies": {
    "@chakra-ui/vue": "^0.11.0",
    "@emotion/css": "^11.7.1",
    "vue": "^3.2.31",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/node": "^16.11.25",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^4.3.8",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.0",
    "typescript": "~4.5.5",
    "vite": "^2.8.4",
    "vue-tsc": "^0.31.4"
  }
}